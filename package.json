{
  "name": "ui-thing",
  "version": "0.0.3",
  "description": "CLI used to add Nuxt 3 components to a project",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ui-thing": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "format": "npx prettier --write .",
    "release": "npm run build && npx changelogen@latest --release && npm publish && git push --follow-tags"
  },
  "keywords": [
    "cli",
    "ui",
    "thing",
    "nuxt3",
    "radix-vue",
    "tailwindcss",
    "nuxtui",
    "shadcn-ui"
  ],
  "author": {
    "name": "Behon Baker",
    "email": "behon.baker@yahoo.com",
    "url": "https://behonbaker.com/"
  },
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "c12": "^1.5.1",
    "commander": "^11.1.0",
    "defu": "^6.1.3",
    "execa": "^8.0.1",
    "figlet": "^1.7.0",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "lodash": "^4.17.21",
    "nypm": "^0.3.3",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/figlet": "^1.5.7",
    "@types/fs-extra": "^11.0.3",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/prompts": "^2.4.7",
    "magicast": "^0.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
